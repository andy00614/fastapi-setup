[run]
source = app
omit = 
    */tests/*
    */migrations/*
    */__init__.py
    */venv/*
    */.venv/*
    */virtualenv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    pass
precision = 2
show_missing = True

[html]
directory = htmlcov